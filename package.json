{
  "name": "route-controller",
  "version": "1.2.0",
  "description": "Simple Express decorator for router",
  "author": "Thada Wangthammang",
  "repository": "mildronize/route-controller",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev": "chokidar \"src/**/*.ts\" -c \"npm link\"",
    "format": "prettier --write \"src/**/*.ts\" ",
    "lint": "tslint -p tsconfig.build.json",
    "test": "jest --config jestconfig.json",
    "test:coverage": "jest --config jestconfig.json --no-cache --coverage",
    "test:watch": "jest --config jestconfig.json --watch",
    "prepare": "run-s build",
    "prepublishOnly": "run-s format lint test build",
    "publish:patch": "npm version patch --force && npm publish",
    "example": "chokidar \"**/*.ts\" -c  \" ts-node example/tester/index.ts\""
  },
  "peerDependencies": {
    "reflect-metadata": ">= 0.1.0 < 0.2.0",
    "express": ">= 4.0.0 < 5.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.15",
    "@types/mock-req-res": "^1.1.3",
    "@types/node": "^14.14.6",
    "chokidar-cli": "^2.1.0",
    "jest": "^26.6.1",
    "mock-req-res": "^1.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "sinon": "^9.2.1",
    "ts-jest": "^26.4.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typeorm": "^0.2.29",
    "typescript": "^4.0.5"
  },
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "Express",
    "Decorator",
    "controller"
  ],
  "dependencies": {
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "connect": "^3.7.0",
    "express": "~4.17.1",
    "http-status-codes": "^2.1.4",
    "reflect-metadata": "^0.1.13",
    "typedi": "^0.8.0"
  }
}
